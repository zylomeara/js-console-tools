{
  "manifest_version": 2,

  "name": "JS Console Tools",
  "description": "JS Console Tools",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "web_accessible_resources": ["script.js"],

  "permissions": ["activeTab"],

  "content_scripts": [{
    "js": ["index.js"],
    "matches": ["*://*/*"]
  }]
}
